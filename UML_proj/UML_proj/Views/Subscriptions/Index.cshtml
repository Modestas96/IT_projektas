@model IEnumerable<UML_proj.Models.Subscription>

@{
    ViewBag.Title = "Index";
}

<h2>Subscription List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Newsletter.newest_message)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Person.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.receit_discord)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.receit_email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Newsletter.newest_message)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Person.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.receit_discord)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.receit_email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id_Subscription }) |
            @Html.ActionLink("Details", "Details", new { id=item.id_Subscription }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id_Subscription })
        </td>
    </tr>
}

</table>
